{
  "expo": {
    "name": "Todo List",
    "slug": "TodoList-react_native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.ganushcorporation.todolist",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.ganushcorporation.todolist",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#464D94"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "An easy and fast todo list",
    "githubUrl": "https://github.com/spikeganush/Todo-list-react-native"
  }
}
